{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "comma-dangle": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "warn",
            "single"
        ],
        "no-console": "warn",
        "no-unused-vars": "warn",
        "semi": [
            "error",
            "always"
        ]
    }
}